      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Story_Of_Abdulla.MainMenu"
      mc:Ignorable="d" 
      d:DesignHeight="1080" d:DesignWidth="1920"
      Title="Setter">



    <Grid>
        <Image Source="/Background/MainBack.jpg" Stretch="Fill"></Image>
        <Grid>


            <StackPanel>

                <Button Content="Назад" Margin="50 50 0 0" HorizontalAlignment="Left" Click="GoBack_Click" x:Name="GoBack" >
                    <Button.Background>
                        <ImageBrush ImageSource="/Story Of Abdulla;component/Background/button.png"/>
                    </Button.Background>
                </Button>


            </StackPanel>

        </Grid>

    </Grid>

</Page>
